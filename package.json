  "scripts": {
    "build:client": "NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js",
    "build": "npm run build:client && npm run build:server",

    "dev:client": "NODE_ENV=development webpack --config webpack.client.js --watch",
    "dev:server": "tsc --noEmit && concurrently \"npm run build:server -- --watch\" \"nodemon dist/server.js\"",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
